{
    "manifest_version": 3,
    "name": "Favorites Manager",
    "description": "Manages your favorites. Icon by Icons8.",
    "version": "1.0",
    "action": {
      "default_icon": "images/icons8-globe-50.png"
    },
    "chrome_url_overrides" : {
      "newtab": "newtab.html"
    },
    "permissions": [
      "bookmarks",
      "storage",
      "unlimitedStorage"
    ],
    "browser_specific_settings": {
      "gecko": {
        "id": "favmanager@neropako.im",
        "strict_min_version": "58.0"
      }
    },
    "background": {
      "scripts": ["background.js"]
    },
    "host_permissions": [
      "*://t0.gstatic.com/*"
    ]
}