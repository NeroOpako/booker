{
    "manifest_version": 3,
    "name": "__MSG_extension_name__",
    "description": "__MSG_extension_desc__",
    "version": "1.2",
    "default_locale" : "en",
    "action": {
      "default_icon": "images/icons8-globe-50.png"
    },
    "chrome_url_overrides" : {
      "newtab": "newtab.html"
    },
    "permissions": [
      "bookmarks",
      "storage",
      "unlimitedStorage"
    ],
    "browser_specific_settings": {
      "gecko": {
        "id": "favmanager@neropako.im",
        "strict_min_version": "58.0"
      }
    },
    "background": {
      "service_worker": "background.js"
    },
    "host_permissions": [
      "*://t0.gstatic.com/*"
    ]
}